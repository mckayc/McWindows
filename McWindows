#Instructions - Paste the following into IE: https://raw.githubusercontent.com/mckayc/McWindows/master/McWindows

Set-WindowsExplorerOptions -EnableShowProtectedOSFiles -EnableShowFileExtensions
Enable-RemoteDesktop
Set-StartScreenOptions -EnableBootToDesktop
 
cinst chocolatey -y
cinst chocolateygui -y
cinst googlechrome -y
cinst visualstudiocode -y

 
Install-WindowsUpdate -AcceptEula
